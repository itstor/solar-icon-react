{
  "version": "1.0.6",
  "author": "Akhmad Thoriq",
  "homepage": "https://github.com/itstor/solar-icon-react#readme",
  "repository": "https://github.com/itstor/solar-icon-react",
  "description": "A set of React components for Solar Icons with 7000+ icons, 37 categories, and 6 styles. Compatible with react-icons IconType type.",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "name": "solar-icon-react",
  "keywords": [
    "solar",
    "icon",
    "icons",
    "react"
  ],
  "scripts": {
    "build": "rollup -c",
    "gen": "tsx scripts/generate.ts",
    "prepublishOnly": "pnpm gen && pnpm build"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/itstor/solar-icon-react/issues"
  },
  "contributors": [
    "Akhmad Thoriq <ahmadthoriq5@gmail.com>"
  ],
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./bd": {
      "import": "./dist/bd/index.mjs",
      "require": "./dist/bd/index.cjs",
      "types": "./dist/bd/index.d.ts"
    },
    "./bo": {
      "import": "./dist/bo/index.mjs",
      "require": "./dist/bo/index.cjs",
      "types": "./dist/bo/index.d.ts"
    },
    "./br": {
      "import": "./dist/br/index.mjs",
      "require": "./dist/br/index.cjs",
      "types": "./dist/br/index.d.ts"
    },
    "./ld": {
      "import": "./dist/ld/index.mjs",
      "require": "./dist/ld/index.cjs",
      "types": "./dist/ld/index.d.ts"
    },
    "./lib": {
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.cjs",
      "types": "./dist/lib/index.d.ts"
    },
    "./li": {
      "import": "./dist/li/index.mjs",
      "require": "./dist/li/index.cjs",
      "types": "./dist/li/index.d.ts"
    },
    "./ol": {
      "import": "./dist/ol/index.mjs",
      "require": "./dist/ol/index.cjs",
      "types": "./dist/ol/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/node": "^22.7.5",
    "@types/react": "^18.0.0",
    "@types/svg-parser": "^2.0.6",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0",
    "rollup-preserve-directives": "^1.1.2",
    "svg-parser": "^2.0.4",
    "ts-node": "^10.8.1",
    "tslib": "^2.7.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
